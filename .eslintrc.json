{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard-with-typescript",
        "plugin:jsx-a11y/recommend",
        "plugin:react-hooks/recommend",
        "plugin:prettier/recommend"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "proyect": "./tsconfig.json"
    },
    "plugins": [
        "react",
        "jsx-a11y",
        "react-hooks",
        "prettier",
        "@typescript-eslint"
    ],
    "rules": {
        "prettier/prettier": "error"
    }
}